	SellingToolsSection:NewToggle("Auto Drop Cash (Ammount Slider Above)", "Toggles On/Off Auto Dropper", function(state)
    if state then
		_G.ToggleDrop = true
			CoreGui:SetCore("SendNotification", {
				Title = "Moe's Hub Message";
				Text = "Toggled On Auto Drop";
				Duration = 2;
			})
        repeat wait(0.1)
		game:GetService("ReplicatedStorage").MainEvent:FireServer("DropMoney", tostring(_G.CashAmount))
		until _G.ToggleDrop == false
    else
			CoreGui:SetCore("SendNotification", {
				Title = "Moe's Hub Message";
				Text = "Toggled Off Auto Drop";
				Duration = 2;
			})
		_G.ToggleDrop = false
    end
	end)

	SellingToolsSection:NewToggle("Prevent CPU Usage (Sellers Only!)", "Lowers Your CPU Useage", function(state)
	if state then
        game:GetService("RunService"):Set3dRenderingEnabled(false)
    CoreGui:SetCore("SendNotification", {
				Title = "Moe's Hub Message";
				Text = "You Now Got More Chanes In Bringing Alt's In";
				Duration = 4;
			})
    else
        game:GetService("RunService"):Set3dRenderingEnabled(true)
    CoreGui:SetCore("SendNotification", {
				Title = "Moe's Hub Message";
				Text = "You Accidently Turned This Off! |Sellers Only!|";
				Duration = 2;
			})
	    end
	end)
	
	SellingToolsSection:NewToggle("Cash Aura", "Cash Aura Pickup.", function(state)
		_G.CashAura = true
		if state then
			_G.CashAura = true
			CoreGui:SetCore("SendNotification", {
				Title = "Moe's Hub Message";
				Text = "Toggled On Cash Aura";
				Duration = 2;
			})

			repeat wait(0.01)
			for i,v in pairs(game.Workspace.Ignored.Drop:GetChildren()) do
				if v.Name == "MoneyDrop" then
				 if (v.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 15 then
				   fireclickdetector(v.ClickDetector)
				 end
				end
			end
			until _G.CashAura == false
		else
			CoreGui:SetCore("SendNotification", {
				Title = "Moe's Hub Message";
				Text = "Toggled Off Cash Aura";
				Duration = 2;
			})
			_G.CashAura = false
		end
	end)
	
	SellingToolsSection:NewToggle("Bot Auto Drop", "Begins A Bot Auto Drop.", function(state)
    if state then
		_G.ToggleBotDrop = true
	CoreGui:SetCore("SendNotification", {
		Title = "Moe's Hub Message";
		Text = "Toggled On Bot Auto Drop.";
		Duration = 5;
	})
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(math.random(-920,-820), -25, -590)
        repeat wait(0.1)
		game:GetService("ReplicatedStorage").MainEvent:FireServer("DropMoney", tostring(10000))
		until _G.ToggleBotDrop == false
    else
	CoreGui:SetCore("SendNotification", {
		Title = "Moe's Hub Message";
		Text = "Toggled Off Bot Auto Drop.";
		Duration = 5;
	})
		_G.ToggleBotDrop = false
    end
    end)
	ScriptsSection:NewButton("Lower GFX", "Turns everything into smooth plastic.", function()
		for _,v in pairs(workspace:GetDescendants()) do
			if v.ClassName == "Part"
				or v.ClassName == "SpawnLocation"
				or v.ClassName == "WedgePart"
				or v.ClassName == "Terrain"
				or v.ClassName == "MeshPart" then
				v.Material = "Plastic"
			end
		end
	end)
	
	ScriptsSection:NewButton("Full Bright", "Makes everything fully bright.", function()
		local s = Instance.new("PointLight", game.Players.LocalPlayer.Character.Head)
			s.Brightness = .3
			s.Range = 100
			game.Lighting.Changed:connect(function()
			game.Lighting.TimeOfDay = "14:00:00"
			game.Lighting.FogEnd = 9999
			game.Lighting.Brightness = 2
			game.Lighting.ColorCorrection.Brightness = 0.1
			game.Lighting.ColorCorrection.Saturation = 0.1
			game.Lighting.Bloom.Intensity = 0.1
			end)
			
	end)
	end
