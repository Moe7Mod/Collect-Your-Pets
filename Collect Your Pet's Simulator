TogglesSection:NewToggle("Auto Fuse Pets", "Auto Fuses Eggs", function(state)
		_G.ToggleEgg = true
		if state then
			_G.ToggleEgg = true
			CoreGui:SetCore("SendNotification", {
				Title = "Moe's Hub Message";
				Text = "Toggled On Buy";
				Duration = 2;
			})
			
			local args = { 
			    [1] = 1
			    
			}
			while wait (1) do
local args = {
    [1] = {
        [1] = true,
        [2] = true,
        [3] = true,
        [4] = true,
        [5] = true,
        [6] = true,
        [7] = true,
        [8] = false
    }
}

game:GetService("ReplicatedStorage").Remotes.AutoFuse:FireServer(unpack(args))

			end
		else
			CoreGui:SetCore("SendNotification", {
				Title = "Moe's Hub Message";
				Text = "Toggled Off Buy";
				Duration = 2;
			})
			_G.ToggleEgg = false
		end
end)

TogglesSection:NewToggle("Egg 1", "Toggle Buy's Egg", function(state)
		_G.ToggleEgg = true
		if state then
			_G.ToggleEgg = true
			CoreGui:SetCore("SendNotification", {
				Title = "Moe's Hub Message";
				Text = "Toggled On Buy";
				Duration = 2;
			})
			
			local args = { 
			    [1] = 1
			    
			}
			while wait (1) do
			game:GetService("ReplicatedStorage").Remotes.BuyEgg:FireServer(unpack(args))
			end
		else
			CoreGui:SetCore("SendNotification", {
				Title = "Moe's Hub Message";
				Text = "Toggled Off Buy";
				Duration = 2;
			})
			_G.ToggleEgg = false
		end
end)

